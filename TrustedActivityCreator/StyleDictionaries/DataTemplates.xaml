<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
					xmlns:v="clr-namespace:TrustedActivityCreator.View"
					xmlns:vm="clr-namespace:TrustedActivityCreator.ViewModel"
					xmlns:m="clr-namespace:TrustedActivityCreator.Model"
                    xmlns:local="clr-namespace:TrustedActivityCreator.StyleDictionaries">

	<DataTemplate DataType="{x:Type vm:ActivityVM}">
		<Canvas>
			<v:ActivityUC/>
		</Canvas>
	</DataTemplate>
	
	<DataTemplate DataType="{x:Type vm:TrustedConditionVM}">
		<Canvas>
			<v:ConditionUC/>
		</Canvas>
	</DataTemplate>

	<DataTemplate DataType="{x:Type vm:EndingPointVM}">
		<Canvas>
			<v:EndingPointUC/>
		</Canvas>
	</DataTemplate>

	<DataTemplate DataType="{x:Type vm:ForkVM}">
		<Canvas>
			<v:ForkUC/>
		</Canvas>
	</DataTemplate>

	<DataTemplate DataType="{x:Type vm:JoinVM}">
		<Canvas>
			<v:JoinUC/>
		</Canvas>
	</DataTemplate>

	<DataTemplate DataType="{x:Type vm:TrustedConnectionVM}">
		<v:TrustedConnectionUC/>
	</DataTemplate>

	<DataTemplate DataType="{x:Type vm:CanvasVM}">
		<v:CanvasUC/>
	</DataTemplate>

	<DataTemplate DataType="{x:Type vm:QuickPanelVM}">
		<v:TrustedQuickPanelUC/>
	</DataTemplate>




</ResourceDictionary>