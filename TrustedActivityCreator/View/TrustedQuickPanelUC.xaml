<UserControl x:Class="TrustedActivityCreator.View.TrustedQuickPanelUC"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:TrustedActivityCreator.View"
			 xmlns:v="clr-namespace:TrustedActivityCreator.View"
			 xmlns:m="clr-namespace:TrustedActivityCreator.Model"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">

	<UserControl.DataContext>
		<m:Activity/>
	</UserControl.DataContext>

	<Grid>
		<Grid.RowDefinitions>
			<RowDefinition Height="*"/>
			<RowDefinition Height="*"/>
		</Grid.RowDefinitions>

		<StackPanel Grid.Row="0" Background="DarkGoldenrod">
			
		</StackPanel>


		<DockPanel Grid.ZIndex="10" Grid.Row="1" Background="Gray">
			
		</DockPanel>
		
	</Grid>
</UserControl>
